# Copy to .env and set your WhatsApp number (digits only, international format)
# Example (Tunisia): 21612345678
VITE_WHATSAPP_PHONE=

# Messenger page username or numeric id (e.g., yourbrand or 123456789)
VITE_MESSENGER_PAGE=

# Preferred channel for primary CTAs: whatsapp | messenger
VITE_PREFERRED_CHANNEL=whatsapp

# Optional: preset id (bridal_luxury | fashion_modern | home_decor | services_minimal)
VITE_PRESET=bridal_luxury
